@{
    Layout = null;
    ViewBag.Title = "Seleccionar Centro de Costo";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" http-equiv="Content-Type" charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="~/Styles/HHMM.css?v=@ViewBag.versionJS" rel="stylesheet" />
</head>
<body style="overflow-y:hidden">
    <div>
        <div class="frm-cuerpo" style="padding:20px 0 10px">
            <div id="divCentroCosto" class="tabla"></div>
            <div class="form-grupo fila centrado">
                <div class="tam-5">
                    <span class="boton" id="btnSeleccionarCentro">Seleccionar</span>
                </div>
                <div class="tam-5">
                    <span class="boton" id="btnCancelarCentro">Cancelar</span>
                </div>
            </div>
        </div>
    </div>
    <input type="hidden" id="hdfPos" value="@ViewBag.Orden" />
    <script src="~/Scripts/CentroCostoLista.js?v=@ViewBag.versionJS"></script>
</body>
</html>

@{
    Layout = null;
    ViewBag.Title = "Seleccionar Medico";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" http-equiv="Content-Type" charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="~/Styles/HHMM.css?v=@ViewBag.versionJS" rel="stylesheet" />
</head>
<body style="overflow-y:hidden">
    <div>
        <div class="frm-cuerpo" style="padding:10px 0 10px">
            <div id="divMedico" class="tabla" style="height:430px;overflow-y:auto"></div>
            <div class="form-grupo fila centrado">
                <div class="tam-5">
                    <span class="boton" id="btnSeleccionarMedico">Seleccionar</span>
                </div>
                <div class="tam-5">
                    <span class="boton" id="btnCancelarMedico">Cancelar</span>
                </div>
            </div>
        </div>
    </div>
	<input type="hidden" id="hdfTipo" value="@ViewBag.tipo" />
    <script src="~/Scripts/MedicoLista.js?v=@ViewBag.versionJS"></script>
</body>
</html>

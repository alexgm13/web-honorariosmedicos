@{
    Layout = null;
    ViewBag.Title = "Seleccionar Medico";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" http-equiv="Content-Type" charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="~/Styles/HHMM.css?v=@ViewBag.versionJS" rel="stylesheet" />
</head>
<body style="overflow-y:hidden">
    <div>
        <div class="frm-cuerpo" style="padding:10px 0 10px">
			<div class="form-grupo fila" style="margin-bottom:20px">
				<div class="tam-13">
                    <span>Registros Selecccionados</span>&nbsp;
					<span id="spnCantidad">0</span>
                </div>
				<div style="float:right">
                <div class="tam-1">
                    <a href="#" class="Icons fa-file-excel-o" id="btnExportar"></a>
                </div>
                <div class="tam-9">
                    <span>Filtrar Seleccionados</span>&nbsp;
					<input type="checkbox" id="chkfiltroseleccionados" />
                </div>
					</div>
            </div>
            <div id="divPrincipal" class="tabla" style="height:350px;overflow-y:auto"></div>
            <div class="form-grupo fila centrado">
                <div class="tam-5">
                    <span class="boton" id="btnGrabar">Grabar</span>
                </div>
                <div class="tam-5">
                    <span class="boton" id="btnCancelar">Cancelar</span>
                </div>
            </div>
        </div>
    </div>
	<input type="hidden" id="hdfDatos" value="@ViewBag.Datos" />
    <script src="~/Scripts/SeleccionMedicoProvision.js?v=@ViewBag.versionJS"></script>
</body>
</html>

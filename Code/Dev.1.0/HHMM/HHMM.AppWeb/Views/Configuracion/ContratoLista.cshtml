@{
    Layout = null;
    ViewBag.Title = "Contrato Médico -Tarifario HHMM";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" http-equiv="Content-Type" charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="~/Styles/HHMM.css?v=@ViewBag.versionJS" rel="stylesheet" />
    <link href="~/Styles/DatePickerX.css?v=@ViewBag.versionJS" rel="stylesheet" />
</head>
<body>
    <div class="frm-cabecera">@ViewBag.Title<a style="float:right;color:white;font-size:16px;display:none" href="~/Files/Formato%20de%20Contratos.xlsx" download"><i class="Icons fa-file-excel-o"></i>&nbsp;Descargar formato de carga</a></div>
    <div class="frm-cuerpo">
        <div>
            <p></p>
        </div>
        <div id="divBusquedaDoctor">
            <div id="divBusqueda" style="text-align: center" class="form-grupo fila">
                <div class="form-grupo fila">
                    <div class="tam-6 izquierda">
                        <span>Sucursal</span>
                    </div>
                    <div class="tam-9">
                        <input type="text" class="lectura" style="width: 100%" id="txtBusquedaSucursal" readonly />
                    </div>
                    <div class="tam-6 izquierda">
                        <span>Medico</span>
                    </div>
                    <div class="tam-18">
                        <input type="text" class="lectura" style="width: 101%" id="txtBusquedaMedico" readonly />
                    </div>
                    <div class="tam-1">
                        <span class="Icons fa-search spnDoctor puntero"></span>
                    </div>
                    <div class="tam-10">
                        <div id="divCheckAutorizacion" style="display:none"><input type="checkbox" id="chkPendienteAut" />&nbsp;Pendientes de autorización</div>
                    </div>
                </div>
                <div class="form-grupo fila">
                    <div class="tam-6 izquierda">
                        <span>Especialidad</span>
                    </div>
                    <div class="tam-9">
                        <select id="ddlBusquedaEspecialidad" style="width: 105%"></select>
                    </div>
                    <div class="tam-6 izquierda">
                        <span>Empresa</span>
                    </div>
                    <div class="tam-18">
                        <input type="text" class="lectura" style="width: 101%" id="txtBusquedaEmpresa" readonly />
                    </div>
                    <div class="tam-1">
                        <span class="Icons fa-search puntero" id="spnEmpresaBusqueda"></span>
                    </div>
                    <div class="tam-10">
                    </div>
                </div>
                <div class="form-grupo fila">
                    <div class="tam-6 izquierda">
                        <span>Código Contrato</span>
                    </div>
                    <div class="tam-9">
                        <input type="text" class="form-texto" style="width: 100%" id="txtBusquedaCodigo" />
                    </div>
                    <div class="tam-6 izquierda">
                        <span>Fecha Inicio</span>
                    </div>
                    <div class="tam-6">
                        <input type="text" class="form-texto" style="width: 100%" id="txtBusquedaFechaInicio" />
                    </div>
                    <div class="tam-4 izquierda">
                        <span>Fecha Fin</span>
                    </div>
                    <div class="tam-6">
                        <input type="text" class="form-texto" style="width: 100%" id="txtBusquedaFechaFin" />
                    </div>
                    <div class="tam-12">
                        <span class="boton" id="btnBusqueda">Buscar</span>&nbsp;<span class="boton" id="btnBusquedaLimpiar">Limpiar</span>
                    </div>
                </div>

            </div>
            <br />
            <div id="divPrincipal" class="tabla" style="display: none">
            </div>
        </div>
        <div id="divDoctor" style="display: none">
            <div id="divDoctorDetalle" style="text-align: center" class="form-grupo fila">
                <div class="form-grupo fila">
                    <div class="tam-7 izquierda">
                        <span>Código Contrato</span>
                    </div>
                    <div class="tam-12">
                        <input type="text" class="lectura" style="width: 50%;left: -55px;position: relative;" id="txtDoctorCodigo" name="limpiar" readonly />
                    </div>
                    <div class="tam-7">
                    </div>
                    <div class="tam-17" style="width:343px">
                    </div>
                    <div class="tam-1" style="padding-left: 25px; padding-right: 0;">
                    </div>
                </div>
                <div class="form-grupo fila">
                    <div class="tam-7 izquierda">
                        <span>Sucursal</span>
                    </div>
                    <div class="tam-12">
                        <input type="text" class="lectura" style="width: 100%" id="txtDoctorSucursal" name="limpiar" readonly />
                    </div>

                    <div class="tam-7">
                        <span>Médico</span>
                    </div>
                    <div class="tam-17" style="width:343px">
                        <input type="text" class="lectura" style="width: 107%" id="txtDoctorEmpresa" name="limpiar" readonly />
                    </div>
                    <div class="tam-1" style="padding-left: 25px; padding-right: 0;">
                        <span class="Icons fa-search spnDoctor puntero"></span>
                    </div>
                </div>
                <div class="form-grupo fila">
                    <div class="tam-7 izquierda">
                        <span>Empresa</span>
                    </div>
                    <div class="tam-12">
                        <input type="text" class="lectura" style="width: 100%" id="txtDoctorEmpSu" name="limpiar" readonly />
                    </div>
                    <div class="tam-7">
                        <span>Fecha Inicio</span>
                    </div>
                    <div class="tam-6">
                        <input type="text" class="form-texto" style="width: 100%" id="txtDoctorFechaInicio" name="limpiar" />
                    </div>
                    <div class="tam-4">
                        <span>Fecha Fin</span>
                    </div>
                    <div class="tam-6">
                        <input type="text" class="form-texto" style="width: 100%" id="txtDoctorFechaFin" name="limpiar" />
                    </div>
                    <div class="tam-1" style="padding-left: 0px;padding-right: 0;">
                    </div>
                </div>
                <div class="form-grupo fila">
                    <div class="tam-7 izquierda">
                        <span style="position: relative; top: -40px">Observaciones</span>
                    </div>
                    <div class="tam-60">
                        <textarea class="form-texto" style="width: 100%; resize: none;" id="txtDoctorObservacion" rows="5" name="limpiar"></textarea>
                    </div>
                    <div class="tam-9" style="top: -40px;left: 18px;width: 195px;" id="divBotonesContrato">
                        <span class="boton VerAc" id="btnDoctorGrabar" style="width: 60px;display: inline-block;">Grabar</span>&nbsp;
                        <span class="boton" id="btnDoctorCancelar" style="width: 60px;display: inline-block;">Regresar</span>
                        <div style="padding-top:15px;display:none" id="divBotonAutorizarRechazar">
                            <span class="boton" id="btnConfRec" style="width: 150px;">Confirm./Rechaz.</span>
                            <span class="boton" id="btnDoctorAutorizar" style="width: 60px;display: none;">Autorizar</span>&nbsp;
                            <span class="boton" id="btnDoctorRechazar" style="width: 60px;display: none;">Rechazar</span>
                        </div>
                    </div>
                </div>
                <div class="form-grupo fila">
                    <div class="tam-80" id="divConfiguracionPago"></div>
                </div>
            </div>
            <br />
            <div class="tab-container">
                <div class="form-group right-abs" style="display:inline-block;float:right">
                    <div class="size-8 left label-control">
                        <div class="spinner" style="display:none" id="spnCargar"></div>
                        <span class="Icons fa-close puntero" style="display:none;color: #00a850;" id="spnEliminarArchivo"></span>
                        <span id="aAdjunto" style="text-decoration:underline;cursor:pointer;color:#00a850"></span>
                        <span class="Icons fa-paperclip puntero" id="spnAdjuntar" style="cursor:pointer;margin-top: 10px;color: #00a850;"></span>&nbsp;&nbsp;
                        <span class="Icons fa-files-o puntero" id="spnCopiar" style="margin-top: 10px;color: #00a850;"></span>
                    </div>
                </div>
                <ul id="ulTabs" class="tabs">
                    <li class="tab-link current" data-tab="tab-1" onclick="mostrarTabs(this,'ulTabs');">Producción Fija</li>
                    <li class="tab-link" data-tab="tab-2" onclick="mostrarTabs(this,'ulTabs');">Producción Escalonada</li>
                    <li class="tab-link" data-tab="tab-3" onclick="mostrarTabs(this,'ulTabs');">Contrato Fijo</li>
                    <li class="tab-link" data-tab="tab-4" onclick="mostrarTabs(this,'ulTabs');">Por Horarios</li>
                    <li class="tab-link" data-tab="tab-5" onclick="mostrarTabs(this,'ulTabs');">Por Turnos</li>
                    <li class="tab-link" data-tab="tab-6" onclick="mostrarTabs(this,'ulTabs');">C. Compartido</li>
                    <li class="tab-link" data-tab="tab-7" onclick="mostrarTabs(this,'ulTabs');">Vacunas</li>
                    <li class="tab-link" data-tab="tab-8" onclick="mostrarTabs(this,'ulTabs');">Inf. Proveedor</li>
                </ul>
                <div id="tab-1" class="tab-content current">
                    <ul id="ulTabsX" class="tabs">
                        <li class="tab-link current" data-tab="tabx-1" onclick="mostrarTabs(this,'ulTabsX');">Configuración</li>
                        <li class="tab-link" data-tab="tabx-2" onclick="mostrarTabs(this,'ulTabsX');">Bonificación</li>
                        <li class="tab-link" data-tab="tabx-3" onclick="mostrarTabs(this,'ulTabsX');">Descuentos</li>
                    </ul>
                    <div id="tabx-1" class="tab-content current">
                        <br />
                        <div id="divProduccionFijoConfiguracion" class="tabla">
                        </div>
                    </div>
                    <div id="tabx-2" class="tab-content">
                        <br />
                        <div id="divProduccionFijoBonificacion" class="tabla">
                        </div>
                    </div>
                    <div id="tabx-3" class="tab-content">
                        <br />
                        <div id="divProduccionFijoDescuento" class="tabla">
                        </div>
                    </div>
                </div>
                <div id="tab-2" class="tab-content">
                    <br />
                    <div id="divProduccionEscalonada" class="tabla">
                    </div>
                </div>
                <div id="tab-3" class="tab-content">
                    <br />
                    <div id="divContratoFijo" class="tabla">
                    </div>
                </div>
                <div id="tab-4" class="tab-content">
                    <ul id="ulTabsY" class="tabs">
                        <li class="tab-link current" data-tab="taby-1" onclick="mostrarTabs(this,'ulTabsY');">Configuración</li>
                        <li class="tab-link" data-tab="taby-2" onclick="mostrarTabs(this,'ulTabsY');">Bonificación</li>
                    </ul>
                    <div id="taby-1" class="tab-content current">
                        <br />
                        <div id="divHorarioCalculo" class="tabla">
                        </div>
                    </div>
                    <div id="taby-2" class="tab-content">
                        <br />
                        <div id="divHorarioBonificacion" class="tabla">
                        </div>
                    </div>
                </div>
                <div id="tab-5" class="tab-content">
                    <ul id="ulTabsZ" class="tabs">
                        <li class="tab-link current" data-tab="tabz-1" onclick="mostrarTabs(this,'ulTabsZ');">Configuración</li>
                        <li class="tab-link" data-tab="tabz-2" onclick="mostrarTabs(this,'ulTabsZ');">Bonificación</li>
                    </ul>
                    <div id="tabz-1" class="tab-content current">
                        <br />
                        <div id="divTurnoCalculo" class="tabla">
                        </div>
                    </div>
                    <div id="tabz-2" class="tab-content">
                        <br />
                        <div id="divTurnoBonificacion" class="tabla">
                        </div>
                    </div>
                </div>
                <div id="tab-6" class="tab-content">
                    <br />
                    <div id="divCompartido" class="tabla">
                    </div>
                </div>
                <div id="tab-7" class="tab-content">
                    <br />
                    <div id="divVacuna" class="tabla">
                    </div>
                </div>
                <div id="tab-8" class="tab-content">
                    <br />
                    <div id="divProveedor" class="tabla">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="PopupMedico" class="PopUp">
        <div style="width: 1000px;margin:3% auto">
            <div class="row">
                <div class="PopupCabecera bg-amarillo center black">
                    <div class="divCerrarPopUp" style="width: 1000px; position: fixed; display: block"><span class="cerrarPopUp" onclick="abrirPopup('PopupMedico');recargarIframe('ifrMedico');"><i class="Icons fa-close"></i></span></div>
                    <h5 style="margin: 10px" id="TituloPopupMedico">SELECCIONAR MÉDICO</h5>
                </div>
                <div class="PopupCuerpo bg-white" style="padding: 15px 20px">
                    <div class="frm-cuerpo-det">
                        <div id="ifrMedico"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="PopupEmpresa" class="PopUp">
        <div style="width: 800px;margin:3% auto">
            <div class="row">
                <div class="PopupCabecera bg-amarillo center black">
                    <div class="divCerrarPopUp" style="width: 800px; position: fixed; display: block"><span class="cerrarPopUp" onclick="abrirPopup('PopupEmpresa');recargarIframe('ifrEmpresa');"><i class="Icons fa-close"></i></span></div>
                    <h5 style="margin: 10px" id="TituloPopupEmpresa">SELECCIONAR EMPRESA</h5>
                </div>
                <div class="PopupCuerpo bg-white" style="padding: 15px 20px">
                    <div class="frm-cuerpo-det">
                        <div id="ifrEmpresa"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="PopupPFConfiguracion" class="PopUp">
        <div style="width: 800px">
            <div class="row">
                <div class="PopupCabecera bg-amarillo center black">
                    <div class="divCerrarPopUp" style="width: 800px; position: fixed; display: block"><span class="cerrarPopUp" onclick="abrirPopup('PopupPFConfiguracion');"><i class="Icons fa-close"></i></span></div>
                    <h5 style="margin: 10px" id="TituloPopupPFConfiguracion">CAMBIAR ESTADO</h5>
                </div>
                <div class="PopupCuerpo bg-white" style="padding: 15px 20px">
                    <div class="frm-cuerpo-det">
                        <div class="form-grupo fila">
                            <div class="tam-6">
                                <span>Fecha Inicio</span>
                            </div>
                            <div class="tam-9">
                                <input type="text" class="form-texto validar" data-secuencia="0" data-validacion="validarFecha|fecha inicio" style="width: 100%" id="txtPFConfiguracionFechaInicio" name="limpiarPFConfiguracion" />
                            </div>
                            <div class="tam-6">
                                <span>Fecha Fin</span>
                            </div>
                            <div class="tam-9">
                                <input type="text" class="form-texto validar" data-secuencia="1" data-validacion="validarFecha|fecha fin" style="width: 100%" id="txtPFConfiguracionFechaFin" name="limpiarPFConfiguracion" />
                            </div>
                        </div>
                        <div class="form-grupo fila">
                            <div class="tam-6">
                                <span>Tipo Atención</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFConfiguracionTipoAtencion" style="width: 105%" name="limpiarPFConfiguracion"></select>
                            </div>
                            <div class="tam-6">
                                <span>T. Admision</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFConfiguracionTipoAdmision" style="width: 105%" name="limpiarPFConfiguracion"></select>
                            </div>
                        </div>
                        <div class="form-grupo fila">
                            <div class="tam-6">
                                <span>Tipo de Paciente</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFConfiguracionTipoPaciente" name="limpiarPFConfiguracion" style="width: 105%"></select>
                            </div>
                            <div class="tam-6">
                                <span>Servicio</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFConfiguracionServicio" style="width: 105%" name="limpiarPFConfiguracion"></select>
                            </div>
                        </div>
                        <div class="form-grupo fila">
                            <div class="tam-6">
                                <span>Aseguradora</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFConfiguracionAseguradora" name="limpiarPFConfiguracion" style="width: 105%"></select>
                            </div>
                            <div class="tam-6">
                                <span>Contrato</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFConfiguracionContrato" name="limpiarPFConfiguracion" style="width: 105%"></select>
                            </div>
                        </div>
                        <div class="form-grupo fila">
                            <div class="tam-6">
                                <span>Especialidad</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFConfiguracionEspecialidad" name="limpiarPFConfiguracion" style="width: 105%"></select>
                            </div>
                            <div class="tam-6">
                                <span>Turno</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFConfiguracionTurno" name="limpiarPFConfiguracion" style="width: 105%"></select>
                            </div>
                        </div>
                        <div class="form-grupo fila">
                            <div class="tam-6">
                                <span>Mod. Facturación</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFConfiguracionModeloFacturacion" name="limpiarPFConfiguracion" style="width: 105%"></select>
                            </div>
                            <div class="tam-6">
                                <span>Aplicación</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFConfiguracionAplicacion" name="limpiarPFConfiguracion" style="width: 105%"></select>
                            </div>
                        </div>
                        <div class="form-grupo fila">
                            <div class="tam-6">
                                <span>Tipo Valor</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFConfiguracionTipoValor" name="limpiarPFConfiguracion" style="width: 105%">
                                    <option value="P">Porcentaje</option>
                                    <option value="F">Factor</option>
                                    <option value="M">Monto Fijo</option>
                                </select>
                            </div>
                            <div class="tam-6">
                                <span>Valor</span>
                            </div>
                            <div class="tam-9">
                                <input type="text" class="form-texto validar" data-secuencia="2" data-validacion="validarTexto|valor mínimo" style="width: 42%" name="limpiarPFConfiguracion" id="txtPFConfiguracionValMin" />&nbsp;
                                <input type="text" class="form-texto validar" data-secuencia="3" data-validacion="validarTexto|valor máximo" style="width: 42%; display: none" name="limpiarPFConfiguracion" id="txtPFConfiguracionValMax" />
                            </div>
                        </div>
                        <div class="form-grupo fila">
                            <div class="tam-6">
                                <span>Prestaciones</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFConfiguracionPrestacion" name="limpiarPFConfiguracion" style="width: 95%">
                                    <option value="T">Todos</option>
                                    <option value="A">Algunos</option>
                                    <option value="E">Exceptos</option>
                                </select>
                                <i class="Icons fa-navicon puntero" style="position: absolute;padding: 8px 5px 0 5px;display:none" id="IcoPFConfiguracion"></i>
                            </div>
                            <div class="tam-6">
                                <span>Tiempo Pago</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFConfiguracionTiempoPago" name="limpiarPFConfiguracion" style="width: 105%"></select>
                            </div>
                        </div>
                        <div class="form-grupo fila">
                            <div class="tam-6">
                                <span>Estado</span>
                            </div>
                            <div class="tam-9">
                                <input type="text" class="lectura" id="txtPFConfiguracionEstado" name="limpiarPFConfiguracion" style="width: 100%" readonly />
                            </div>
                            <span class="historial puntero" style="display:none;text-decoration:underline;color:#00a850" id="PFConfiguracionHistorial" onclick="verHistorial('MedicoContratoDetalle')">Historial de Cambios</span>
                        </div>
                        <br />
                        <div class="form-grupo fila centrado">
                            <div class="tam-5 desControl">
                                <span class="boton" id="btngrabarPFConfiguracion">Grabar</span>
                            </div>
                            <div class="tam-5">
                                <span class="boton desControlCancelar" onclick="abrirPopup('PopupPFConfiguracion')">Cancelar</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="PopupPFBonificacion" class="PopUp">
        <div style="width: 800px">
            <div class="row">
                <div class="PopupCabecera bg-amarillo center black">
                    <div class="divCerrarPopUp" style="width: 800px; position: fixed; display: block"><span class="cerrarPopUp" onclick="abrirPopup('PopupPFBonificacion');"><i class="Icons fa-close"></i></span></div>
                    <h5 style="margin: 10px" id="TituloPopupPFBonificacion">CAMBIAR ESTADO</h5>
                </div>
                <div class="PopupCuerpo bg-white" style="padding: 15px 20px">
                    <div class="frm-cuerpo-det">
                        <div class="form-grupo fila">
                            <div class="tam-6">
                                <span>Operador</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFBonificacionOperador" style="width: 105%" name="limpiarPFBonificacion">
                                    <option value="S">+:Suma</option>
                                    <option value="R">-:Resta</option>
                                </select>
                            </div>
                            <div class="tam-6">
                                <span>Fecha</span>
                            </div>
                            <div class="tam-9">
                                <input type="text" class="form-texto validar" data-secuencia="0" data-validacion="validarFecha|fecha inicio" style="width: 42%" id="txtPFBonificacionFechaInicio" placeholder="Inicio" name="limpiarPFBonificacion" />&nbsp;
                                <input type="text" class="form-texto validar" data-secuencia="1" data-validacion="validarFecha|fecha fin" style="width: 42%" id="txtPFBonificacionFechaFin" placeholder="Fin" name="limpiarPFBonificacion" />
                            </div>
                        </div>
                        <div class="form-grupo fila">
                            <div class="tam-6">
                                <span>T. Bonificación</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFBonificacionTipoBonificacion" style="width: 105%" name="limpiarPFBonificacion" class="validar" data-secuencia="2" data-validacion="validarCombo|tipo bonificacion"></select>
                            </div>

                            <div class="tam-6">
                                <span>Mod. Facturación</span>
                            </div>
                            <div class="tam-9">
                                <select id="ddlPFBonificacionModeloFacturacion" name="limpiarPFBonificacion" style="width: 105%"></select>
                            </div>

                        </div>
                        <div class="form-grupo fila">
                            <div class="tam-6">
                                <span>T. Admision</span>
                            </div>
